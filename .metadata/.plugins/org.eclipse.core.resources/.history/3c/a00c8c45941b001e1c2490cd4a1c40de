package com;

import java.util.Scanner;

public class ej11 {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("Teclea el ancho y largo ");
		int tam = sc.nextInt();
//		System.out.println("Teclea el largo: ");
//		int largo = sc.nextInt();

		for (int i = 1; i <= tam; i++) {
			
			for (int j = 1; j <= tam; j++) {
				System.out.println(" * ");
			}
				
			    System.out.println();

		}
	}

}
