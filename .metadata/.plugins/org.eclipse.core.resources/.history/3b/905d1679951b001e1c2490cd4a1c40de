package com;

import java.util.Scanner;

public class ej11 {
	public static void main(String[] args) {
		



		Scanner sc = new Scanner(System.in);
		System.out.println("Teclea el ancho  ");
		int tam = sc.nextInt();	
		System.out.println("Teclea el largo ");
		int tam1 = sc.nextInt();	

		for (int i = 1; i <= tam; i++) {
			
			for (int j = 1; j <= tam1; j++) 
				System.out.print(" * ");			
			    System.out.println();

		}
	}

}
